{
  "name": "@aragon/cli",
  "version": "6.2.5",
  "description": "Aragon command-line tools",
  "main": "dist/cli.js",
  "bin": {
    "aragon": "./dist/cli.js",
    "dao": "./aliases/dao"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "compile-src": "babel src --out-dir dist --source-maps --copy-files",
    "extract-roles": "node scripts/extract-roles",
    "build": "npm run extract-roles && npm run compile-src",
    "build:watch": "npm run build -- -- --watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava --verbose",
    "test:watch": "ava --watch",
    "test:coverage": "nyc --all --reporter=text --reporter=text-summary --reporter=lcovonly --exclude 'config/**' --exclude '**/*.test.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/aragon-cli"
  },
  "keywords": [
    "cli",
    "aragon",
    "ethereum"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "contributors": [
    "Jorge Izquierdo <jorge@aragon.one>",
    "Luis Cuende <luis@aragon.one>",
    "Oliver Nordbjerg <oliver@aragon.one>"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aragon/aragon-cli/issues"
  },
  "homepage": "https://github.com/aragon/aragon-cli#readme",
  "dependencies": {
    "@aragon/apm": "^3.1.0",
    "@aragon/aragen": "^5.0.0",
    "@aragon/cli-utils": "^0.0.8",
    "@aragon/wrapper": "5.0.0-rc.9",
    "byte-size": "^5.0.1",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "dev-null": "^0.1.1",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-ens": "^0.7.7",
    "execa": "^2.0.3",
    "fs-extra": "^8.1.0",
    "get-folder-size": "^2.0.1",
    "git-clone": "^0.1.0",
    "go-platform": "^1.0.0",
    "ignore": "^4.0.6",
    "inquirer": "^6.5.1",
    "internal-ip": "^4.3.0",
    "js-sha3": "^0.8.0",
    "listr": "^0.14.3",
    "listr-input": "0.1.3",
    "open": "^6.0.0",
    "replace": "^1.1.0",
    "semver": "^6.1.1",
    "source-map-support": "^0.5.13",
    "tmp-promise": "^2.0.1",
    "truffle": "4.1.14",
    "truffle-flattener": "^1.2.9",
    "web3": "^1.2.0",
    "web3-eth-abi": "^1.2.0",
    "web3-utils": "^1.2.0",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@aragon/apps-agent": "^1.1.1",
    "@aragon/apps-finance": "^3.0.0",
    "@aragon/apps-payroll": "1.0.0-rc.1",
    "@aragon/apps-survey": "^1.0.0",
    "@aragon/apps-token-manager": "^2.0.0",
    "@aragon/apps-vault": "^4.0.0",
    "@aragon/apps-voting": "^2.0.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "ava": "^2.3.0",
    "babel-eslint": "^10.0.2",
    "core-js": "^3.1.4",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.1"
  },
  "aragon": {
    "clientVersion": "4d766c906f5e113cf27212186768b2a72f186d9c",
    "clientPort": "3000",
    "defaultGasPrice": "2",
    "defaultIpfsTimeout": "300000"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3",
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "files": [
      "src/**/*.test.js"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "standard",
      "plugin:ava/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  }
}
