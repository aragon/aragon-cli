{
  "name": "@aragon/cli",
  "version": "5.9.2",
  "description": "Aragon command-line tools",
  "main": "dist/cli.js",
  "bin": {
    "aragon": "./dist/cli.js",
    "dao": "./aliases/dao"
  },
  "scripts": {
    "postinstall": "npm run clean-modules && npm run fix-lockfile",
    "clean-modules": "del-cli './node_modules/**/.git'",
    "fix-lockfile": "node scripts/fix-lockfile",
    "prepare": "npm run build",
    "link": "npm link",
    "unlink": "npm unlink",
    "compile-src": "babel src --out-dir dist --source-maps --copy-files",
    "extract-roles": "node scripts/extract-roles",
    "build": "npm run extract-roles && npm run compile-src",
    "build:watch": "npm run build -- -- --watch",
    "lint": "eslint src test/commands && documentation lint src",
    "test": "nyc ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/aragon-cli"
  },
  "keywords": [
    "cli",
    "aragon",
    "ethereum"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "contributors": [
    "Jorge Izquierdo <jorge@aragon.one>",
    "Luis Cuende <luis@aragon.one>",
    "Oliver Nordbjerg <oliver@aragon.one>"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aragon/aragon-cli/issues"
  },
  "homepage": "https://github.com/aragon/aragon-cli#readme",
  "dependencies": {
    "@aragon/apm": "^3.1.0",
    "@aragon/apps-shared-minime": "^1.0.1",
    "@aragon/aragen": "^4.0.0",
    "@aragon/os": "^4.0.0",
    "@aragon/wrapper": "^5.0.0-rc.3",
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.6.2",
    "byte-size": "^5.0.1",
    "chalk": "^2.1.0",
    "cli-table": "^0.3.1",
    "colors": "^1.2.4",
    "dev-null": "^0.1.1",
    "eth-ens-namehash": "^2.0.8",
    "execa": "^1.0.0",
    "find-up": "^4.0.0",
    "fs-extra": "^8.0.1",
    "git-clone": "^0.1.0",
    "go-ipfs": "^0.4.18-hacky2",
    "ignore": "^4.0.6",
    "ipfs-api": "^26.1.2",
    "ipfs-http-client": "^31.0.0",
    "js-sha3": "^0.8.0",
    "listr": "^0.14.3",
    "listr-input": "0.1.3",
    "listr-silent-renderer": "^1.1.1",
    "listr-update-renderer": "^0.5.0",
    "listr-verbose-renderer": "^0.5.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "opn": "^6.0.0",
    "replace": "^1.1.0",
    "semver": "^6.0.0",
    "source-map-support": "^0.5.11",
    "stringify-tree": "^1.0.2",
    "tmp-promise": "^1.1.0",
    "truffle": "4.1.14",
    "truffle-flattener": "^1.2.9",
    "truffle-hdwallet-provider": "^1.0.2",
    "truffle-hdwallet-provider-privkey": "^0.3.0",
    "web3": "1.0.0-beta.35",
    "yargs": "^12.0.0"
  },
  "devDependencies": {
    "@aragon/apps-finance": "^3.0.0",
    "@aragon/apps-token-manager": "^2.0.0",
    "@aragon/apps-vault": "^4.0.0",
    "@aragon/apps-voting": "^2.0.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.0",
    "del-cli": "^2.0.0",
    "documentation": "^11.0.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.1",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  },
  "aragon": {
    "clientVersion": "e08deb62080e17fe652fa83f60dc81a02c455c41",
    "clientPort": "3000",
    "defaultGasPrice": "10000000000",
    "defaultIpfsTimeout": "300000"
  },
  "yargs": {
    "short-option-groups": true,
    "camel-case-expansion": true,
    "dot-notation": true,
    "parse-numbers": false,
    "boolean-negation": true
  },
  "greenkeeper": {
    "ignore": [
      "truffle",
      "web3",
      "yargs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "documentation lint",
        "git add"
      ]
    }
  }
}
