{
  "name": "@aragon/cli",
  "version": "6.2.5",
  "description": "Aragon command-line tools",
  "main": "dist/cli.js",
  "bin": {
    "aragon": "./dist/cli.js",
    "dao": "./aliases/dao"
  },
  "scripts": {
    "postinstall": "npm run clean-modules",
    "clean-modules": "del-cli './node_modules/**/.git'",
    "prepare": "npm run build",
    "link": "npm link",
    "unlink": "npm unlink",
    "compile-src": "babel src --out-dir dist --source-maps --copy-files",
    "extract-roles": "node scripts/extract-roles",
    "build": "npm run extract-roles && npm run compile-src",
    "build:watch": "npm run build -- -- --watch",
    "lint": "eslint src test/commands && documentation lint src",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:coverage": "nyc --all --reporter=text --reporter=text-summary --reporter=lcovonly --exclude 'config/**' --exclude '**/*.test.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/aragon-cli"
  },
  "keywords": [
    "cli",
    "aragon",
    "ethereum"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "contributors": [
    "Jorge Izquierdo <jorge@aragon.one>",
    "Luis Cuende <luis@aragon.one>",
    "Oliver Nordbjerg <oliver@aragon.one>"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aragon/aragon-cli/issues"
  },
  "homepage": "https://github.com/aragon/aragon-cli#readme",
  "dependencies": {
    "@aragon/apm": "^3.1.0",
    "@aragon/apps-shared-minime": "^1.0.1",
    "@aragon/aragen": "^5.0.0",
    "@aragon/cli-utils": "^0.0.8",
    "@aragon/id": "^2.1.0",
    "@aragon/os": "^4.0.0",
    "@aragon/wrapper": "5.0.0-rc.9",
    "ajv": "^6.6.2",
    "byte-size": "^5.0.1",
    "chalk": "^2.1.0",
    "cli-table": "^0.3.1",
    "del-cli": "^2.0.0",
    "dev-null": "^0.1.1",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-ens": "^0.7.7",
    "execa": "^1.0.0",
    "find-up": "^4.0.0",
    "fs-extra": "^8.0.1",
    "get-folder-size": "^2.0.1",
    "git-clone": "^0.1.0",
    "go-platform": "^1.0.0",
    "ignore": "^4.0.6",
    "inquirer": "^6.4.1",
    "internal-ip": "^4.3.0",
    "ipfs-api": "^26.1.2",
    "ipfs-http-client": "^33.0.1",
    "js-sha3": "^0.8.0",
    "listr": "^0.14.3",
    "listr-input": "0.1.3",
    "lodash.merge": "^4.6.1",
    "node-fetch": "^2.3.0",
    "open": "^6.0.0",
    "public-ip": "^3.1.0",
    "replace": "^1.1.0",
    "semver": "^6.1.1",
    "source-map-support": "^0.5.11",
    "stringify-tree": "^1.0.2",
    "tmp-promise": "^2.0.1",
    "truffle": "4.1.14",
    "truffle-flattener": "^1.2.9",
    "truffle-hdwallet-provider": "^1.0.2",
    "truffle-hdwallet-provider-privkey": "^0.3.0",
    "web3": "^1.2.0",
    "web3-eth-abi": "^1.2.0",
    "web3-utils": "^1.2.0",
    "which": "^1.3.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@aragon/apps-agent": "^1.1.1",
    "@aragon/apps-finance": "^3.0.0",
    "@aragon/apps-token-manager": "^2.0.0",
    "@aragon/apps-vault": "^4.0.0",
    "@aragon/apps-voting": "^2.0.0",
    "@aragon/apps-payroll": "1.0.0-rc.1",
    "@aragon/apps-survey": "^1.0.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^2.1.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.1.4",
    "documentation": "^12.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3",
    "proxyquire": "^2.1.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.3.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ava": {
    "require": [
      "core-js",
      "regenerator-runtime",
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  },
  "aragon": {
    "clientVersion": "4d766c906f5e113cf27212186768b2a72f186d9c",
    "clientPort": "3000",
    "defaultGasPrice": "2",
    "defaultIpfsTimeout": "300000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "documentation lint",
      "git add"
    ]
  }
}
