{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "no-console": [false],
      "no-empty": [false],
      "interface-name": [false],
      "arrow-parens": [false],
      "object-literal-sort-keys": [false],
      "ordered-imports": [false]
    },
    "rulesDirectory": []
}
