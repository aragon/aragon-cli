{
  "name": "create-aragon-app",
  "version": "2.2.14",
  "description": "Aragon command-line tool to create aragon apps",
  "main": "dist/cli.js",
  "bin": {
    "create-aragon-app": "./dist/cli.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "link": "npm link",
    "unlink": "npm unlink",
    "build": "babel src --out-dir dist --source-maps",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint src test && documentation lint src",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:coverage": "nyc --all --reporter=text --reporter=text-summary --reporter=lcovonly --exclude 'config/**' --exclude '**/*.test.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/create-aragon-app"
  },
  "keywords": [
    "create-aragon-app",
    "aragon",
    "ethereum"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "contributors": [
    "Gabriel Garcia <gabrielpk.18@gmail.com>"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aragon/aragon-cli/issues"
  },
  "homepage": "https://github.com/aragon/aragon-cli#readme",
  "dependencies": {
    "chalk": "^2.1.0",
    "execa": "^1.0.0",
    "figures": "^3.0.0",
    "fs-extra": "^8.0.1",
    "git-clone": "^0.1.0",
    "listr": "^0.14.3",
    "listr-silent-renderer": "^1.1.1",
    "listr-update-renderer": "^0.5.0",
    "listr-verbose-renderer": "^0.5.0",
    "replace": "^1.1.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.1.4",
    "documentation": "^12.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-ava": "^7.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3",
    "regenerator-runtime": "^0.13.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ava": {
    "require": [
      "core-js",
      "regenerator-runtime",
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "documentation lint",
      "git add"
    ]
  }
}
