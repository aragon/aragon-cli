{
  "name": "@aragon/node-api",
  "version": "0.0.1",
  "description": "Programmatic interface for interacting with Aragon",
  "main": "index.js",
  "scripts": {
    "pretest": "node scripts/setup-integration-tests.js & sleep 20",
    "posttest": "node scripts/teardown-integration-tests.js",
    "test": "ava --verbose || (npm run posttest && exit 42)",
    "test:watch": "ava --watch",
    "test:clean": "rm -rf ./.tmp",
    "test:coverage": "nyc --all --reporter text --reporter text-summary --reporter lcovonly  npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/node-api"
  },
  "keywords": [
    "aragon",
    "cli",
    "ethereum"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@aragon/apps-shared-minime": "^1.0.2",
    "@aragon/os": "^4.3.0",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "ava": "^2.4.0",
    "babel-plugin-istanbul": "^5.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0"
  },
  "dependencies": {},
  "ava": {
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "scripts/**/*.js"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3",
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
