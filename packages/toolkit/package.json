{
  "name": "@aragon/toolkit",
  "version": "0.0.5",
  "description": "Programmatic interface for interacting with Aragon",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/aragon-cli/tree/master/packages/toolkit"
  },
  "scripts": {
    "generate": "typechain --target web3-v1 --outDir typechain './node_modules/@aragon/abis/os/artifacts/*.json'",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "build:docs": "typedoc --out docs --allowJs --readme none --theme minimal --mode file",
    "lint": "eslint src test --ext '.js,.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava --verbose",
    "test:watch": "ava --watch",
    "test:clean": "rm -rf ./.tmp && npm run test:reset-cache",
    "test:reset-cache": "ava --reset-cache",
    "test:update-snapshots": "npm run test:clean && npm run test -- --update-snapshots",
    "test:coverage:html": "nyc --reporter html  npm run test",
    "test:coverage": "nyc --reporter text --reporter text-summary --reporter lcovonly  npm run test",
    "test:integration": "ava --verbose ./test-examples/*.test.js"
  },
  "keywords": [
    "aragon",
    "cli",
    "ethereum"
  ],
  "files": [
    "@types",
    "config",
    "schemas",
    "dist"
  ],
  "author": "Aragon Association <legal@aragon.org>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@aragon/abis": "^1.1.0",
    "@aragon/apps-shared-minime": "^1.0.2",
    "@aragon/aragen-snapshot": "^0.0.3",
    "@aragon/truffle-config-v5": "^1.0.0",
    "@aragon/wrapper": "^5.0.0-rc.23",
    "ajv": "^6.11.0",
    "chalk": "^3.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-ens": "^0.8.0",
    "ethers": "^4.0.44",
    "find-up": "^4.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "rxjs": "^6.5.3",
    "semver": "^7.1.3",
    "solidity-parser-antlr": "^0.4.11",
    "valid-url": "1.0.9",
    "web3": "^1.2.6",
    "web3-core": "^1.2.6",
    "web3-core-helpers": "^1.2.6",
    "web3-eth-abi": "^1.2.6",
    "web3-utils": "^1.2.6"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@aragon/contract-helpers-test": "^0.0.2",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/lodash.flatten": "^4.4.6",
    "@types/node": "^12.12.24",
    "@types/node-fetch": "^2.5.4",
    "@types/rx": "^4.1.1",
    "@types/semver": "^7.1.0",
    "@types/sinon": "^7.5.2",
    "@types/valid-url": "^1.0.2",
    "@types/which": "^1.3.2",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "bignumber.js": "^9.0.0",
    "bn.js": "^4.11.8",
    "core-js": "^3.6.4",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-ava": "^10.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "sinon": "^9.0.0",
    "proxyquire": "^2.1.0",
    "tmp": "^0.1.0",
    "ts-node": "^8.6.2",
    "typechain": "^1.0.5",
    "typechain-target-web3-v1": "^1.0.4",
    "typedoc": "^0.16.11",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.test.{js,ts}"
    ],
    "extensions": [
      "ts"
    ]
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.{js,ts}"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3",
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  }
}
